<?xml version="1.0" encoding="utf-8" ?> 
<resourcePack xmlns="http://www.MobileHold.cz/ResourcePack.xsd" name="testRP2">
  <description>Second test pack to test multiple resource packs</description>
  <tileTypes>
    <tileType name="Sand">
      <texturePath>sand.jpg</texturePath>
      <movementSpeed>0.5</movementSpeed>
    </tileType>
  </tileTypes>
  <unitTypes>
    <unitType name="TestUnit">
      <iconPath>sand.jpg</iconPath>
      <modelPath>Box.mdl</modelPath>
      <assemblyPath>DefaultPackage.dll</assemblyPath>
      <extension>
        <projectileType>testRP2/TestProjectile</projectileType>
      </extension>
    </unitType>
    <unitType name="TestWorker">
      <iconPath>sand.jpg</iconPath>
      <modelPath>Box.mdl</modelPath>
      <assemblyPath>DefaultPackage.dll</assemblyPath>
    </unitType>
  </unitTypes>
  <projectileTypes>
    <projectileType name="TestProjectile">
      <modelPath>Box.mdl</modelPath>
      <speed>30</speed>
      <size x="1" y="0.5"/>
      <assemblyPath>DefaultPackage.dll</assemblyPath>
    </projectileType>
  </projectileTypes>
  <buildingTypes>
    <buildingType name="TestBuilding">
      <iconPath>sand.jpg</iconPath>
      <modelPath>Box.mdl</modelPath>
      <size x="3" y="3"/>
      <assemblyPath>DefaultPackage.dll</assemblyPath>
      <extension>
        <workerType>testRP2/TestWorker</workerType>
      </extension>
    </buildingType>
  </buildingTypes>
</resourcePack>
