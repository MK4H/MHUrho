<?xml version="1.0" encoding="utf-8" ?> 
<gamePack xmlns="http://www.MobileHold.cz/GamePack.xsd" name="testRP2">
  <description>Second test pack to test multiple resource packs</description>
  <levels>
    <dataDirPath>Levels</dataDirPath>
  </levels>
  <levelLogicTypes>
    <levelLogicType name="TestLogic" ID="1">
      <assemblyPath>DefaultPackage.dll</assemblyPath>
    </levelLogicType>
    <levelLogicType name="TestLogic2" ID="2">
      <assemblyPath>DefaultPackage.dll</assemblyPath>
    </levelLogicType>
  </levelLogicTypes>
	<playerAITypes>
		<playerAIType name="TestAI" ID="1" category="ai">
			<iconTextureRectangle left="0" top="0" right="50" bottom="50"/>
			<assemblyPath>DefaultPackage.dll</assemblyPath>
		</playerAIType>
	  <playerAIType name="testHuman1" ID="2" category="human">
	    <iconTextureRectangle left="0" top="50" right="50" bottom="100"/>
	    <assemblyPath>DefaultPackage.dll</assemblyPath>
	  </playerAIType>
	  <playerAIType name="testNeutral1" ID="3" category="neutral">
	    <iconTextureRectangle left="0" top="0" right="50" bottom="50"/>
	    <assemblyPath>DefaultPackage.dll</assemblyPath>
	  </playerAIType>
	  <playerAIType name="testNeutral2" ID="4" category="neutral">
	    <iconTextureRectangle left="0" top="50" right="50" bottom="100"/>
	    <assemblyPath>DefaultPackage.dll</assemblyPath>
	  </playerAIType>
	</playerAITypes>
  <resourceTypes></resourceTypes>
  <tileTypes>
    <defaultTileType name="Default" ID="1">
	    <iconTextureRectangle left="0" top="100" right="100" bottom="200"/>
      <texturePath>xamarin.png</texturePath>
			<minimapColor R="102" G="194" B="255"/>
	    <manuallySpawnable>true</manuallySpawnable>
    </defaultTileType>
    <tileType name="Sand" ID="2">
	    <iconTextureRectangle left="0" top="0" right="100" bottom="100"/>
      <texturePath>sand.jpg</texturePath>
	    <minimapColor R="255" G="210" B="77"/>
	    <manuallySpawnable>true</manuallySpawnable>
    </tileType>
  </tileTypes>
  <unitTypes>
    <unitType name="TestUnit" ID="1">
      <model>
				<modelPath>Box.mdl</modelPath>
	      <scale x="1" y="1" z="1"/>
	    </model>
      <material>
				<materialPath>Green.xml</materialPath>
			</material>
      <assemblyPath>DefaultPackage.dll</assemblyPath>
      <extension>
        <projectileType>TestProjectile</projectileType>
      </extension>
	    <iconTextureRectangle left="0" top="0" right="100" bottom="100"/>
	    <manuallySpawnable>true</manuallySpawnable>
    </unitType>
    <unitType name="TestWorker" ID="9">
      <model>
				<modelPath>Box.mdl</modelPath>
	      <scale x="1" y="0.5" z="1"/>
      </model>
      <material>
				<materialPath>StoneMaterial.xml</materialPath>
      </material>
      <assemblyPath>DefaultPackage.dll</assemblyPath>
	    <iconTextureRectangle left="0" top="100" right="100" bottom="200"/>
	    <manuallySpawnable>false</manuallySpawnable>
    </unitType>
    <unitType name="Chicken" ID="3">
      <model>
				<modelPath>Chicken/Models/Chicken.mdl</modelPath>
	      <scale x="0.1" y="0.1" z="0.1"/>
      </model>
			<material>
				<materialListPath>Chicken/Models/Chicken.txt</materialListPath>
			</material>
      <assemblyPath>DefaultPackage.dll</assemblyPath>
	    <iconTextureRectangle left="0" top="200" right="100" bottom="300"/>
			<manuallySpawnable>true</manuallySpawnable>
    </unitType>
  </unitTypes>
  <projectileTypes>
    <projectileType name="TestProjectile" ID="1">
      <model>
				<modelPath>Box.mdl</modelPath>
	      <scale x="0.2" y="0.2" z="0.8"/> 
      </model>
      <material>
				<materialPath>StoneMaterial.xml</materialPath>
			</material>
      <assemblyPath>DefaultPackage.dll</assemblyPath>
			<extension>
				<speed>30</speed>
			</extension>
    </projectileType>
		<projectileType name="EggProjectile" ID="2">
			<model>
				<modelPath>Models/Sphere.mdl</modelPath>
				<scale x="0.2" y="0.2" z="0.3"/>
			</model>
			<material>
				<materialPath>EggMaterial.xml</materialPath>
			</material>
			<assemblyPath>DefaultPackage.dll</assemblyPath>
			<extension>
				<speed>10</speed>
			</extension>
		</projectileType>
  </projectileTypes>
  <buildingTypes>
    <buildingType name="TestBuilding" ID="1">
      <model>
				<modelPath>Box.mdl</modelPath>
	      <scale x="3" y="3" z="3"/>
      </model>
      <material>
				<materialPath>StoneMaterial.xml</materialPath>
	    </material>
      <assemblyPath>DefaultPackage.dll</assemblyPath>
      <extension>
        <workerType>TestWorker</workerType>
        <tileType>Sand</tileType>
      </extension>
	    <iconTextureRectangle left="0" top="300" right="100" bottom="400"/>
	    <manuallySpawnable>true</manuallySpawnable>
	    <size x="3" y="3"/>
    </buildingType>
  </buildingTypes>
	<resourceIconTexturePath>Textures/tileIcons.png</resourceIconTexturePath>
	<tileIconTexturePath>Textures/tileIcons.png</tileIconTexturePath>
	<unitIconTexturePath>Textures/entityIcons.png</unitIconTexturePath>
	<buildingIconTexturePath>Textures/entityIcons.png</buildingIconTexturePath>
	<playerIconTexturePath>Textures/tileIcons.png</playerIconTexturePath>
</gamePack>
